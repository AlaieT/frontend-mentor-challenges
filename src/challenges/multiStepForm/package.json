{
  "name": "main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./scripts/vite/vite.config.ts",
    "build": "tsc && vite build --config ./scripts/vite/vite.config.ts",
    "preview": "vite preview --config ./scripts/vite/vite.config.ts",
    "type": "tsc --noEmit",
    "prettier:fix": "npx prettier -w ./",
    "style": "stylelint **/*.scss",
    "style:fix": "yarn style --fix",
    "lint": "eslint -c .eslintrc --ext .ts,.tsx --no-eslintrc ./src",
    "lint:fix": "yarn lint --fix",
    "test": "jest --config ./scripts/jest/jest.config.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.46",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "postcss-scss": "^4.0.6",
    "sass": "^1.59.3",
    "stylelint": "^15.6.3",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0"
  }
}
